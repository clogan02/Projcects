	public int x, y;
	public int speed;
	public int spriteCounter = 0, spriteNumber = 1;
	
	public String direction;
	
	public BufferedImage walkUp1, walkUp2, walkDown1, walkDown2, walkLeft, walkRight, idleUp, idleDown, idleLeft, idleRight;
}
